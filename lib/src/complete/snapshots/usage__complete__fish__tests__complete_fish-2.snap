---
source: lib/src/complete/fish.rs
assertion_line: 95
expression: "complete_fish(&CompleteOptions\n{\n    usage_bin: \"usage\".to_string(), shell: \"fish\".to_string(), bin:\n    \"mycli\".to_string(), cache_key: Some(\"1.2.3\".to_string()), spec: None,\n    usage_cmd: Some(\"mycli complete --usage\".to_string()),\n    include_bash_completion_lib: false, source_file: None,\n})"
---
# @generated by usage-cli from usage spec

# if "usage" is not installed show an error
if ! type -p usage &> /dev/null
    echo >&2
    echo "Error: usage CLI not found. This is required for completions to work in mycli." >&2
    echo "See https://usage.jdx.dev for more information." >&2
    return 1
end

if ! set -q _usage_spec_mycli_1_2_3
  set -g _usage_spec_mycli_1_2_3 (mycli complete --usage | string collect)
end
# Use a content-addressable temp file to avoid "argument list too long" error
set -l tmpdir (if set -q TMPDIR; echo $TMPDIR; else; echo /tmp; end)
set -l spec_file "$tmpdir/usage__usage_spec_mycli_1_2_3.spec"
if not test -f "$spec_file"
    echo $_usage_spec_mycli_1_2_3 > "$spec_file"
end

set -l tokens
if commandline -x >/dev/null 2>&1
    complete -xc mycli -a '(command usage complete-word --shell fish -f "$spec_file" -- (commandline -xpc) (commandline -t))'
else
    complete -xc mycli -a '(command usage complete-word --shell fish -f "$spec_file" -- (commandline -opc) (commandline -t))'
end
